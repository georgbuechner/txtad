{
  "id": "general",
  "name": "General",
  "description": "holds general game-handlers",
  "shared": true,
  "priority":15,
  "permeable": true,
  "listeners": [
    {
      "id": "L1", 
      "re_event": "#ctx replace *rooms -> .*", 
      "arguments": "#> {ctx.*rooms->description}", 
      "permeable": true
    },
    {
      "id": "L2", 
      "re_event": "^(?!#)(.*)", 
      "arguments": "#> <i>#event</i>", 
      "permeable": true
    }
  ]
}
